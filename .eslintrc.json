{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
      "import/imports-first": 0,
      "no-unused-vars": 1,
      "no-console": 0,
      "no-underscore-dangle": 0,
      "arrow-parens": 0,
      "no-param-reassign": 0,
      "padded-blocks": 0,
      "prefer-const": 1,
      "linebreak-style":0,
      "max-len":0,
      "func-names": 0
    }
}
